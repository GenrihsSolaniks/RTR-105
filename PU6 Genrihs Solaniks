N1
#include <stdio.h>
void skaitlu_paarbaude();

int main()
{
skaitlu_paarbaude(); //bez argumentiem
return 0; //bez return
}
void skaitlu_paarbaude()
{
int n, i, flag=0;
printf("Ievadiet pozit朝vu veselu skaitli: ");
scanf("%d",&n);
for(i=2; i<=n/2; i++)
{
if(n%i==0)
{
flag=1;
}
}
if (flag==1)
printf(" %d nav pamata skaitlis.", n);
else
printf(" %d ir pamata skaitlisr.", n);
}

N2
#include <stdio.h>
void priekshskatiijuma_un_displeja_paarbaude(int n);

int main()
{
int n;
printf("Ievadiet pozit朝vu veselu skaitli: ");
scanf("%d",&n);//n-arguments
priekshskatiijuma_un_displeja_paarbaude(n);
return 0;//bez return
}
void priekshskatiijuma_un_displeja_paarbaude(int n)
{
int i, flag = 0;
for(i=2; i<=n/2; ++i)
{
if(n%i==0)
{
flag=1;
break;
}
}
if(flag == 1)
printf(" %d nav pamata skaitlis.",n);
else
printf(" %d ir pamata skaitlis.", n);
}

N3
#include <stdio.h>
int ieguut_veselu_skaitli();

int main()
{
int n, i, flag=0; //bez argumentiem
n = ieguut_veselu_skaitli();
for(i=2; i<=n/2; ++i)
{
if(n%i==0){
flag = 1;
break;
}
}
if (flag == 1)
printf(" %d nav pamata skaitlis.", n);
else
printf(" %d ir pamata skaitlis.", n);
return 0;
}//ar return
int ieguut_veselu_skaitli()
{
int n;
printf("Ievadiet pozit朝vu veselu skaitli: ");
scanf("%d",&n);
return n;
}

N4
#include <stdio.h>
int skaitlu_paarbaude(int n);

int main()
{
int n, flag;
printf("Ievadiet pozit朝vu veselu skaitli: ");
scanf("%d",&n); //n-arguments un ar return
flag = skaitlu_paarbaude(n);
if(flag == 1)
printf(" %d nav pamata skaitlis",n);
else
printf(" %d ir pamata skaitlis",n);
return 0;
}
int skaitlu_paarbaude(int n)
{
int i;
for(i=2; i<=n/2; i++)
{
if(n%i == 0)
return 1;
}
return 0;
}
